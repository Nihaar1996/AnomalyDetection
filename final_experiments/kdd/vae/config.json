{
    "seed_runs": 5,
    "lr_scheduler": {
        "args": {
            "gamma": 0.999,
            "step_size": 50
        },
        "type": "StepLR"
    },
    "trainer": {
        "epochs": 4,
        "save_dir": "saved/",
        "monitor": "min val_loss",
        "early_stop": 40,
        "tensorboard": true,
        "save_period": 5,
        "verbosity": 2
    },
    "name": "test",
    "optimizer": {
        "args": {
            "amsgrad": false,
            "weight_decay": 0,
            "lr": 0.0005
        },
        "type": "Adam"
    },
    "seed": 0,
    "loss": "vae_loss",
    "kl_strategy": {
        "C_start": 0,
        "loss": "weight",
        "n_cycle": 6,
        "recon_loss": "bce",
        "ratio": 0.8,
        "C_stop": 25,
        "beta_start": 0.0,
        "gamma": 1000,
        "schedule": "cyclical",
        "beta_stop": 0.01
    },
    "metrics": [],
    "data_loader": {
        "args": {
            "num_workers": 1,
            "scaling": "binary",
            "batch_size": 1000,
            "dataset": "kdd",
            "shuffle": true,
            "data_dir": "data/"
        },
        "type": "AnomDataLoader"
    },
    "arch": {
        "args": {
            "batch_norm": false,
            "z_size": 32,
            "num_flows": 20,
            "made_h_size": 380,
            "dropout": 0.2,
            "decoder_sizes": [
                64,
                128
            ],
            "encoder_sizes": [
                128,
                64
            ]
        },
        "type": "VAE"
    },
    "n_gpu": 1
}